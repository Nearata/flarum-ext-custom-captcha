{
    "name": "nearata/flarum-ext-custom-captcha",
    "description": "Custom Captcha for Flarum",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "Unlicense",
    "require": {
        "php": "^8.0",
        "flarum/core": "^1.8"
    },
    "require-dev": {
        "laravel/pint": "^1.5.0"
    },
    "authors": [
        {
            "name": "Nearata",
            "email": "nearata@protonmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nearata\\CustomCaptcha\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Custom Captcha",
            "category": "feature",
            "icon": {
                "name": "fas fa-robot",
                "color": "#fff",
                "backgroundColor": "#20A4F3"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Nearata\\CustomCaptcha\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "pint src"
    }
}
